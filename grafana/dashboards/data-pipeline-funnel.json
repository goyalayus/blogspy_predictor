  "panels": [
    {
      "gridPos": { "h": 2, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": { "content": "# BlogSpy: Data Pipeline Funnel", "mode": "markdown" },
      "type": "text"
    },
    {
      "title": "Top of Funnel: Link Discovery Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 2 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(blogspy_links_discovered_total[5m]))",
          "legendFormat": "Discovered Links / sec"
        }
      ]
    },
    {
      "title": "Stage 1: Classification Funnel (jobs/sec)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": {
        "defaults": {
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 2 },
      "id": 3,
      "options": {
        "displayMode": "lcd",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": [ "lastNotNull" ], "fields": "", "values": false },
        "showUnfilled": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(blogspy_jobs_processed_total{type=\"classification\"}[5m]))",
          "legendFormat": "Input Rate"
        },
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(blogspy_jobs_processed_total{type=\"classification\", outcome=\"classified_blog\"}[5m]))",
          "legendFormat": "Outcome: Blog"
        },
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(blogspy_jobs_processed_total{type=\"classification\", outcome=\"classified_irrelevant\"}[5m]))",
          "legendFormat": "Outcome: Irrelevant"
        },
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(blogspy_jobs_processed_total{type=\"classification\", outcome=\"failure\"}[5m]))",
          "legendFormat": "Outcome: Failure"
        }
      ]
    },
    {
      "title": "Netloc Classification Cache",
      "type": "piechart",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 10 },
      "id": 4,
      "options": {
        "displayLabels": [ "name", "percent" ],
        "legend": { "displayMode": "list", "placement": "right" },
        "pieType": "pie",
        "reduceOptions": { "calcs": [ "sum" ], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (status) (rate(blogspy_netloc_cache_total[5m]))",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "title": "Stage 2: Content Quality Analysis",
      "type": "piechart",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 10 },
      "id": 5,
      "options": {
        "displayLabels": [ "name", "percent" ],
        "legend": { "displayMode": "list", "placement": "right" },
        "pieType": "donut",
        "reduceOptions": { "calcs": [ "sum" ], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (decision) (rate(blogspy_content_storage_decisions_total[5m]))",
          "legendFormat": "{{decision}}"
        }
      ]
    },
    {
      "title": "Bottom of Funnel: Data Growth",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 6,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "blogspy_urls_total{job=\"go-reaper\"}",
          "legendFormat": "Total URLs Discovered"
        },
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "blogspy_urls_in_content_table_total{job=\"go-reaper\"}",
          "legendFormat": "Total Content Entries Stored (Est.)"
        }
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "tags": [ "blogspy", "pipeline" ],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
