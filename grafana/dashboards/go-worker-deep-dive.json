{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "gridPos": { "h": 2, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": { "content": "# Go Worker & Reaper: Deep Dive", "mode": "markdown" },
      "type": "text"
    },
    {
      "title": "Job Processing Rate (jobs/sec)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 2 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (type, outcome) (rate(blogspy_jobs_processed_total[5m]))",
          "legendFormat": "{{type}} - {{outcome}}"
        }
      ]
    },
    {
      "title": "Active Worker Goroutines",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 2 },
      "id": 3,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (type) (blogspy_job_queue_active_workers)",
          "legendFormat": "{{type}} workers"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "title": "Job Duration (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le, type) (rate(blogspy_job_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{type}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Dependency Call Duration (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 5,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le, dependency) (rate(blogspy_dependency_call_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{dependency}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Reaper Activity",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "id": 6,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (type) (rate(reaper_jobs_reset_total[5m]))",
          "legendFormat": "{{type}} jobs reset / sec"
        }
      ]
    },
    {
      "title": "Links Skipped Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "id": 7,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (reason) (rate(blogspy_links_skipped_total[5m]))",
          "legendFormat": "Reason: {{reason}}"
        }
      ]
    },
    {
      "title": "Bloom Filter False Positive Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "id": 8,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "(sum(rate(blogspy_bloom_filter_checks_total{status=\"false_positive\"}[5m])) / sum(rate(blogspy_bloom_filter_checks_total{status=\"hit\"}[5m]))) * 100",
          "legendFormat": "False Positive Rate"
        }
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "tags": [ "blogspy", "go", "deep-dive" ],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
